{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "NODE_ENV=production next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production ts-node server.ts",
    "docker:dev:build": "docker-compose --env-file=./.env.development build",
    "docker:dev:start": "docker-compose --env-file=./.env.development up",
    "docker:dev:launch": "docker-compose --env-file=./.env.development up -d --build",
    "docker:stage:build": "docker-compose --env-file=./.env.production build",
    "docker:stage:start": "docker-compose --env-file=./.env.production up",
    "docker:mongo": "docker-compose up mongodb",
    "test": "mocha --require ts-node/register --file test/api/populate.ts --recursive --extension test.ts --timeout 5000",
    "postman": "newman run test/postman.json",
    "lint": "next lint --fix"
  },
  "dependencies": {
    "@emotion/cache": "^11.6.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@emotion/utils": "^1.0.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/core": "^5.0.0-alpha.54",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.49",
    "@mui/material": "^5.0.1",
    "@mui/styles": "^5.0.1",
    "@mui/system": "^5.1.0",
    "@types/mongoose": "^5.11.97",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "clsx": "^1.1.1",
    "cookie": "^0.4.1",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "mongodb": "^4.1.4",
    "mongoose": "^6.0.7",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.43.4",
    "scss": "^0.2.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.22",
    "@types/cookie": "^0.4.1",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/newman": "^5.1.4",
    "@types/react": "17.0.24",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "newman": "^5.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "4.4.3"
  }
}